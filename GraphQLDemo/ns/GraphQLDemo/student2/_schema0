type Query {
 getStudent(id: ID):Student
}

type Student {
	id: ID
	firstName: String
	lastName: String
	email: Email
	address: Address
	phone: Phone
}

type Email {
	email: String
}

type Address {
	line1: String
	line2: String
	city: String
	state: String
	zipcode: String
}
type Phone {
	mobile: String
	home: String	
}
